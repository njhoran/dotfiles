urxvt*geometry:     	110x28
urxvt*saveLines:    	12000
urxvt*foreground:	#C4C0AB
urxvt*depth:		32
urxvt*background:	rgba:1111/1111/1111/dddd

urxvt*font:		xft:dina:pixelsize=10:antialias=true:autohinting=true
urxvt*boldFont:		xft:dina:pixelsize=10:antialias=true:autohinting=true
Xft.dpi:	     	96
Xft.antialias:	    	true
Xft.rgba:		rgb
Xft.hinting:		true
Xft.hintstyle:		hintslight

